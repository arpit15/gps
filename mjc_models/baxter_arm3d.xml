                                            <site name="leg_top" pos="0 0 0.15" size="0.01" />

                                            <body name="ball" pos="0 0 0">
                                                <geom name="ball_geom" rgba="0.8 0.6 0.6 1" type="cylinder" fromto="0 0 -0.15 0 0 0.15" size="0.028" density="2000" contype="2" conaffinity="1" />
                                            </body>
                                        </body>

                                        <body name="right_gripper_base" pos="0 0 0.025">
                                            <geom name="gf3" type="capsule" fromto="0 0 0 0.09137 0.00495 0" size="0.01" />

                                            <body name="r_gripper_l_finger" pos="0 -0.0015 0.02">
                                                <geom name="gf4" type="capsule" fromto="0 0 0 0.09137 0.0 0" size="0.01" />
                                                <!-- <joint name="r_gripper_l_finger_joint" pos="0 0 0" axis="0 1 0" type="slide" range="0 0.020833" /> -->
                                                
                                                <body name="r_gripper_l_finger_tip" pos="0 0.01725 0.1127">

                                                </body>
                                            </body>

                                            <body name="r_gripper_r_finger" pos="0 0.0015 0.02">
                                                <geom name="gf5" type="capsule" fromto="0 0 0 0.09137 0.0 0" size="0.01" />
                                                <!-- <joint name="r_gripper_r_finger_joint" pos="0 0 0" axis="0 1 0" type="slide" range="-0.020833 0" /> -->

                                                <body name="r_gripper_r_finger_tip" pos="0 -0.01725 0.1127">
                                                    
                                                </body>
                                            </body>

                                        </body>

                                        
                                    </body>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
      
        <body name="w1" pos="0.0 0.15 -0.35">
            <geom name="w1" rgba="0.2 0.2 0.2 1" type="box" size="0.05 0.05 0.05" contype="1" conaffinity="1" />
        </body>

        <body name="w2" pos="0.0 0.3 -0.45">
            <geom name="w2" rgba="0.2 0.2 0.2 1" type="box" size="0.3 0.3 0.05" contype="1" conaffinity="1" />
        </body>

    </worldbody>

    <actuator>
        <motor joint="right_s0" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_s1" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_e0" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_e1" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_w0" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_w1" ctrlrange="-100.0 100.0" ctrllimited="true" />
        <motor joint="right_w2" ctrlrange="-100.0 100.0" ctrllimited="true" />
    </actuator>

</mujoco>
