            <geom name="e1p" type="sphere" rgba="0.1 0.1 0.1 1" pos="-0.06 0.09 0.2" size="0.03" />
            <geom name="e2p" type="sphere" rgba="0.1 0.1 0.1 1" pos=" 0.06 0.09 0.2" size="0.03" />
            <geom name="sp" type="capsule" fromto="0 0 -0.4 0 0 0.2" size="0.1" />
            <joint name="right_s0" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-1.70168 1.70168"/>

            <body name="right_upper_shoulder" pos="0.055695 0 0.011038">
                <geom name="sl" type="capsule" fromto="0 -0.1 0 0 0.1 0" size="0.02" />
                <joint name="right_s1" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-2.147 1.047"/>

                <body name="right_lower_shoulder" pos="0.069 0 0.27035" quat="0.707107 -0.707107 0 0">
                    <geom name="uar" type="capsule" fromto="-0.1 0 0 0.1 0 0" size="0.02" />
                    <joint name="right_e0" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-3.05418 3.05418"/>

                    
                    <body name="right_lower_elbow" pos="0.069 0 0.26242" quat="0.5 -0.5 -0.5 -0.5">
                        <geom name="ef" type="capsule" fromto="0 -0.02 0 0.0 0.02 0" size="0.02" />
                        <joint name="right_e1" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-0.05 2.618"/>

                        <body name="right_upper_forearm" pos="0.10359 0 0" quat="0.5 0.5 0.5 0.5">
                            <geom name="fr" type="capsule" fromto="-0.1 0 0 0.1 0 0" size="0.02" />
                            <joint name="right_w0" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-3.059 3.059"/>

                            
                            <body name="right_lower_forearm" pos="0.01 0 0.2707" quat="0.5 -0.5 -0.5 -0.5">
