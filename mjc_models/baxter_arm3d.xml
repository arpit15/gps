                                <joint name="right_w1" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-1.5708 2.094"/>

                                <body name="right_wrist" pos="0.115975 0 0" quat="0.5 0.5 0.5 0.5">
                                    <geom name="wr" type="capsule" fromto="-0.02 0 0 0.02 0 0" size="0.01" />
                                    <joint name="right_w2" type="hinge" limited="true" pos="0 0 0" axis="0 0 1" range="-3.059 3.059"/>

                                    <body name="right_hand" pos="0 0 0.11355">
